{
  "name": "reactjs-browserify-starter",
  "version": "0.5.0",
  "description": "reactjs browserify, with tape, starter ",
  "repository": {
    "type": "git",
    "url": "https://github.com/griffio/reactjs-browserify-starter.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "watchify --outfile 'testling | faucet' --external react/addons --external react/lib/ReactContext --external react/lib/ExecutionEnvironment ./node_modules/babel-polyfill test.jsx",
    "bundle": "browserify --external react --external react-dom --entry index.jsx --outfile bundle.js --debug",
    "preserve": "browserify --require react --require react-dom > react-bundle.js",
    "serve": "budo index.jsx:bundle.js --open -- --plugin livereactload --external react --external react-dom"
  },
  "author": "griffio",
  "license": "ISC",
  "dependencies": {
    "aphrodite": "^2.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.1.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "1.1.1",
    "livereactload": "3.3.0",
    "react-proxy": "^2.0.8",
    "react-test-renderer": "^16.2.0",
    "tape": "^4.9.0",
    "uglifyify": "^4.0.5",
    "watchify": "3.11.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "testling": {
    "browsers": [
      "chrome/latest",
      "safari/5..latest"
    ],
    "files": "test.jsx"
  }
}
