{
  "name": "reactjs-browserify-starter",
  "version": "0.2.0",
  "description": "reactjs browserify",
  "repository": {
    "type": "git",
    "url": "https://github.com/griffio/reactjs-browserify-starter.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "browserify test.jsx | testling -x open",
    "bundle": "browserify --entry index.jsx --outfile bundle.js --debug",
    "serve": "beefy index.jsx:bundle.js --open"
  },
  "author": "griffio",
  "license": "ISC",
  "dependencies": {
    "react": "^0.13.3"
  },
  "devDependencies": {
    "browserify": "^11.2.0",
    "reactify": "^1.1.1",
    "tape": "^4.2.0",
    "uglifyify": "^3.0.1"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "testling": {
    "files": "test.jsx"
  }
}
